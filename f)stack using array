//Stack using Array
#include<stdio.h>

struct stack{
    int size;
    int top;
    int *arr;
};

int main(){
    // struct stack s;
    // s.size =80;
    // s.top =-1 ;//-1 means stack is empty
    // s.arr = (int*) malloc(s.size * sizeof(int*));
    // return 0; 
    struct stack *s;
    s->size =80;
    s->top =-1 ;//-1 means stack is empty
    s->arr = (int*) malloc(s->size * sizeof(int*));
    return 0; 
}